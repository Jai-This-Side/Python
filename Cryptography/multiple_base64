import base64

cipherStr = input("enter the cipher text")
roundsInt = int(input("till how many rounds you want to continuously decode it"))

def multiple_base64(cipher, rounds):
    for i in range(rounds):
        cipher = cipher.decode('utf-8')
        cipher_bytes = base64.b64decode(cipher)
        print(f"Text after round {i}: {cipher_bytes}")
        cipher = cipher_bytes

multiple_base64(cipherStr, roundsInt)